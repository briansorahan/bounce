{
  "name": "bounce",
  "version": "1.0.0",
  "description": "Node.js TypeScript REST API wrapper for FluCoMa audio analysis library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:native": "node-gyp rebuild",
    "build:ts": "tsc && cp src/openapi.json dist/openapi.json",
    "build": "npm run build:native && npm run build:ts",
    "clean": "node-gyp clean && rm -rf dist build",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "tsx src/test.ts"
  },
  "keywords": [
    "flucoma",
    "audio",
    "analysis",
    "dsp",
    "onset",
    "rest",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^12.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "express": "^5.2.1",
    "redoc-express": "^2.1.3",
    "wav-decoder": "^1.3.0"
  }
}
